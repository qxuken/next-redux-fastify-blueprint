{
  "name": "next-redux-fastify-blueprint",
  "repository": "https://github.com/qxuken/next-redux-fastify-blueprint",
  "engines": {
    "node": ">=11.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint -c ./.eslintrc.js --ext .js,.jsx,.ts,.tsx --fix",
      "git add -A"
    ]
  },
  "scripts": {
    "dev": "NODE_ENV=development nodemon --max_old_space_size=4096 ./backend/index.js",
    "prettier": "prettier --config ./.prettierrc  --write './{pages,backend,src,utils}/**/*.{js,jsx,ts,tsx}'",
    "lint": "eslint -c ./.eslintrc.js --ext .js,.jsx,.ts,.tsx .",
    "lint:write": "eslint -c ./.eslintrc.js --ext .js,.jsx,.ts,.tsx --fix .",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production node ./backend/index.js",
    "build-n-start": "yarn build && yarn start"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@types/lodash": "^4.14.123",
    "@types/next": "^8.0.3",
    "@types/node": "^11.11.6",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "@types/react-redux": "^7.0.5",
    "@types/redux": "^3.6.0",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "autoprefixer": "^9.4.10",
    "axios": "^0.18.0",
    "babel-plugin-inline-dotenv": "^1.2.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "babel-plugin-transform-imports": "^1.5.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "css-mqpacker": "^7.0.0",
    "dotenv": "^7.0.0",
    "fastify": "^2.0.1",
    "fastify-healthcheck": "^2.0.0",
    "fastify-metrics": "^4.0.1",
    "fastify-rate-limit": "^2.1.0",
    "fastify-redis": "^3.0.0",
    "ioredis": "^4.2.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "next": "^8.0.3",
    "node-sass": "^4.10.0",
    "postcss-color-function": "^4.0.1",
    "postcss-css-reset": "^1.0.2",
    "postcss-csso": "^3.0.0",
    "postcss-discard-comments": "^4.0.2",
    "postcss-import": "^12.0.1",
    "postcss-initial": "^3.0.0",
    "postcss-media-minmax": "^4.0.0",
    "postcss-nested": "^4.1.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-css": "^1.0.2",
    "redux-devtools-extension": "^2.13.5",
    "redux-form": "^8.1.0",
    "redux-logger": "^3.0.6",
